    "express"
  ],
  "engines": {
    "node": ">=10.x"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "configure-env": "^1.0.0",
    "cookie-parser": "~1.4.5",
    "debug": ">=4.3.1",
    "express": "^4.17.1",
    "express-handlebars": "^6.0.6",
    "morgan": "^1.10.0",
    "serve-favicon": "^2.5.0",
